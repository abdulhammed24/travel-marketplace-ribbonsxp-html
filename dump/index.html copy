    <script>
      $(".travel-deals").owlCarousel({
        loop: false,
        slideBy: 1,
        dots: false,
        nav: true,
        navText: [
          `<i class="fa-sharp fa-solid fa-chevron-left text-base"></i>`,
          `<i class="fa-sharp fa-solid fa-chevron-right text-base"></i>`,
        ],
