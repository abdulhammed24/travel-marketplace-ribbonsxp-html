          0: {
            items: 1,
            margin: 5,
          },
          640: {
            items: 2,
            margin: 10,
          },
          1024: {
            items: 3,
            nav: true,
            margin: 20,
          },
        },
      });

      $(".how-it-works").owlCarousel({
        loop: false,
        slideBy: 1,
        margin: 20,
        dots: false,
        nav: true,
